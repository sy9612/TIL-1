import java.util.Scanner;

public class 점프점프_11060 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int N = sc.nextInt();
		int[] arr = new int[N];
		
		for (int i = 0; i < N; i++) {
			arr[i] = sc.nextInt();
		}
		
		int now = 0;
		
		while(now < N) {
			int Max = 0;
			for (int i = 0; i < arr[now]; i++) {
				Max = Math.max(max, arr[now+i])
			}
		}
	}
}
